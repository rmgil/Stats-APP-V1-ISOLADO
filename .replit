externalPort = 3001

[[ports]]
localPort = 41753
externalPort = 4200

[[ports]]
localPort = 43401
externalPort = 3003

[[ports]]
localPort = 46871
externalPort = 3000

[agent]
integrations = ["styling_replit:1.0.0", "flask_supabase:1.0.0"]
expertMode = true

